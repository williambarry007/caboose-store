<% _.each(options, function(option, index) { %>
  <select id="<%= 'option' + (index + 1) %>" name="<%= option.name %>">
    <option value="">Choose an Option</option>
    
    <% _.each(option.values, function(value) { %>
      <option value="<%= value %>"><%= value %></option>
    <% }); %>
  </select>
<% }); %>

